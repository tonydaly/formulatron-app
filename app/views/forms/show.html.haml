%h2 Responses

%table
  %thead
    %tr
      %th #
      - @qualitative_elements.collect(&:label).each do |element_label|
        %th
          = element_label
      %th Date
  %tbody
    - @submissions.each_with_index do |submission, index|
      %tr
        %td=index+1
        - @qualitative_elements.collect(&:name).each do |name|
          %td
            = submission.values.where(name: name).first
        %td=submission.created_at.to_s(:long_ordinal)

= render partial: 'quantitative_element', collection: @quantitative_elements, :as => :element

= link_to 'Back to forms', forms_path